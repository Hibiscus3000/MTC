vardef = var:v '=' rational:r {Pair(:v,:r)};
rational = int:i '/' natural:n {Rational(:i,:n)} | int:i {Rational(:i,1)};
int = sp* '-' natural:n {minus(:n)} | natural:n {:n};
natural = sp* digit+$d sp* {s2i($d)};
var = sp* (letter (letter | digit)*)$str sp* {Var($str)};
letter = 'a' - 'z' | 'A' - 'Z';
digit = '0'-'9';
sp = ' ';